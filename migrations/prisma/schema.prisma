generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Ping {
    id         String   @id
    service    String
    amount     Int
    created_at DateTime @default(now())
    updated_at DateTime @default(now()) @updatedAt

    @@map("pings")
}
